<%= link_to('back to list', lists_path) %>

<h1>Create List</h1>

<div class="lists new">
<%= form_for(@list, :url => lists_path, :method => 'post') do |f| %>

    <% @list.errors.full_messages_for(:title).each do |message| %>
      <div><%= message %></div>
    <% end %>

  <%= render(:partial => 'form', :locals => {:f => f}) %>

  <div class="form-buttons">
    <%= f.submit('Create List') %>
  </div>
<% end %>
</div>
