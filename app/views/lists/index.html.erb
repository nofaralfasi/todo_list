<p><%= link_to('Add a new list', new_list_path, :class => 'action new') %></p>

<h1>Lists:</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Action</th>
  </tr>

  <% @lists.each do |list| %>
    <tr>
      <th><%= list.title %></th>
      <th><%= link_to('Show', list_path(list)) %></th>
      <th><%= link_to('Edit', edit_list_path(list)) %></th>
    </tr>

  <% end %>
</table>

